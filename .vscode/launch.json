{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch hardhat compile",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/hardhat/internal/cli/cli.js",
        "args": ["compile"],
        "cwd": "${workspaceFolder}/hardhat",
        "protocol": "inspector",
        "runtimeExecutable": null,
        "env": {},
        "sourceMaps": true,
        "outFiles": ["${workspaceFolder}/hardhat/build/**/*.js"]
      }
    ]
  }
  